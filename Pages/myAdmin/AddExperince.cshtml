@page
@model Portfolioo.Pages.myAdmin.AddExperinceModel
@{
}
<div class="container">
    <form method="post">
        <div class="row justify-content-center">
            <div class="col-md-4">
                <h1>
                    Add-Experince
                </h1>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">JobTittle</label>
                <input class="form-control" asp-for="experince.JobTittle">
            </div>
         <div class="row justify-content-center">
            <div class="col-md-4">
                 <label class="form-label">Location</label>
                 <input class="form-control" asp-for="experince.Location">
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Start-Year</label>
                <input class="form-control" asp-for="experince.StartYear">
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">End-Year</label>
                <input class="form-control" asp-for="experince.EndYear">
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <input type="submit" value="Add Experince" class="btn btn-primary" />
            </div>
        </div>
    </form>
</div>

