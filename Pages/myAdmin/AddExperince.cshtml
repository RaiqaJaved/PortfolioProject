@page
@model Portfolioo.Pages.myAdmin.AddExperinceModel
@{
}
<div class="container">
    <form method="post">
        <div class="row justify-content-center">
            <div class="col-md-4">
                <h1>
                    Add-Experince
                </h1>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">JobTittle</label>
                <input class="form-control" asp-for="experince.JobTittle">
                <span asp-validation-for="experince.JobTittle" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Start-Year</label>
                <input class="form-control" asp-for="experince.StartYear">
                <span asp-validation-for="experince.StartYear" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">End-Year</label>
                <input class="form-control" asp-for="experince.EndYear">
                <span asp-validation-for="experince.EndYear" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Location</label>
                <input class="form-control" asp-for="experince.Location">
                <span asp-validation-for="experince.Location" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Description</label>
                <input class="form-control" asp-for="experince.Desc">
                <span asp-validation-for="experince.Desc" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <input type="submit" value="Add Experince" class="btn btn-primary" />
            </div>
        </div>
    </form>
</div>
