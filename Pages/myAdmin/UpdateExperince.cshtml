@page
@model Portfolioo.Pages.myAdmin.Update_ExperinceModel
@{
}
<div class="container">
    <form method="post">
        <div class="row justify-content-center">
            <div class="col-md-4">
                <h1>
                    Update-Experince
                </h1>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">job Tittle</label>
                <input class="form-control" asp-for="experince.JobTittle">
                <input class="form-control" hidden asp-for="experince.Id">
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Location</label>
                <input class="form-control" asp-for="experince.Location">
            </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Start-Year</label>
                <input class="form-control" asp-for="experince.StartYear">
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">End-Year</label>
                <input class="form-control" asp-for="experince.EndYear">
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Description</label>
                <input class="form-control" asp-for="experince.Desc">
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <input type="submit" value="Update Experince" class="btn btn-primary" />
            </div>
        </div>
    </form>
</div>

