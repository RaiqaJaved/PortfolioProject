@page
@model Portfolioo.Pages.myAdmin.AddSocialNetworksModel
@{
}
<div class="container">
    <form method="post">
        <div class="row justify-content-center">
            <div class="col-md-4">
                <h1>
                    Add Social Networks
                </h1>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Facebook</label>
                <input class="form-control" asp-for="socialNetworks.Facebook">
                <span asp-validation-for="socialNetworks.Facebook" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Twitter</label>
                <input class="form-control" asp-for="socialNetworks.Twitter">
                <span asp-validation-for="socialNetworks.Twitter" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Linkdin</label>
                <input class="form-control" asp-for="socialNetworks.LinkedIn">
                <span asp-validation-for="socialNetworks.LinkedIn" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Institute-Instragram</label>
                <input class="form-control" asp-for="socialNetworks.Instragram">
                <span asp-validation-for="socialNetworks.Instragram" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">skyp</label>
                <input class="form-control" asp-for="socialNetworks.Skyp">
                <span asp-validation-for="socialNetworks.Skyp" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <input type="submit" value="Add SocialNetworks" class="btn btn-primary" />
            </div>
        </div>
    </form>
</div>
