@page
@model Portfolioo.Pages.myAdmin.Add_SkillsModel
@{
}
<div class="container">
    <form method="post">
        <div class="row justify-content-center">
            <div class="col-md-4">
                <h1>
                    Add-Skills
                </h1>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Name</label>
                <input class="form-control" asp-for="skills.Name">
                <span asp-validation-for="skills.Name" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Percentage</label>
                <input class="form-control" asp-for="skills.Percentage">
                <span asp-validation-for="skills.Percentage" class="text-danger"></span>
            </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <input type="submit" value="Add Skills" class="btn btn-primary" />
            </div>
        </div>
    </form>
</div>
